    "format:oxlint": "bun run lint:oxlint --fix",
    "format:prettier": "prettier --write .",
    "lint": "bun run lint:oxlint && bun run lint:prettier",
    "format": "bun run format:oxlint && bun run format:prettier",
    "vale": "vale readme.md src/",
    "prepare": "husky",
    "knip": "knip"
  },
